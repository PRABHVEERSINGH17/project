#include<stdio.h>
int main(){
  char ch;
  printf("Enter the charcharacter:");
  scanf("%c",&ch);
  if(ch>=65 && ch<=90){
    printf("The character is uppercase:");
  }
  if(ch>97 && ch<=122){
    printf("The character is lowercase:");
  }
  if(ch>48 && ch<=57){
    printf("The character is a digit:");
  }
  if((ch>=0 && ch<=47) || (ch>=58 && ch<=64) || (ch>=91 && ch<=96) || (ch>=123 && ch<=127)){
    printf("The letter is special symbol:");
  }
  return 0;
}
Enter the charcharacter:23 
The character is a digit: 
Enter the charcharacter:A
The character is uppercase: 
