#include <stdio.h>

int main() {
    int num, reversed = 0, temp;

    printf("Enter a 5-digit number: ");
    scanf("%d", &num);

    temp = num;

    while(temp!= 0) {
        int digit = temp % 10;
        reversed = reversed * 10 + digit;
        temp /= 10;
    }

    printf("Reversed number: %d\n", reversed);

    return 0;
}#include <stdio.h>

int main() {
    int num, reversed = 0, temp;

    printf("Enter a 5-digit number: ");
    scanf("%d", &num);

    temp = num;

    while(temp!= 0) {
        int digit = temp % 10;
        reversed = reversed * 10 + digit;
        temp /= 10;
    }

    printf("Reversed number: %d\n", reversed);

