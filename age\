#include<stdio.h>
int main(){
  char sex,ms;
  int age;
  printf("Enter the sex(M/F) and ms and age");
  scanf("%d %c %c",&age,&sex,&ms);
  if((ms=='M') || (ms=='U'&& sex=='M'&&age>=30)||(ms=='U'&&sex=='F'&&age>=25))
    printf("Driver should be insured\n");
  else
    printf("Driver should not b e insured\n");
  return 0;
}
